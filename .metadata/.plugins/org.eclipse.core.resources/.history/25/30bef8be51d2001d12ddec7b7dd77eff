#pragma once

#include <vector>

class AnalogOut{
private:
	TIM_HandleTypeDef* timer;
	uint32_t channel;
	GPIO_TypeDef* port;
	uint16_t pin;

public:
	static vector<AnalogOut> all_analog_outs;

	AnalogOut(TIM_HandleTypeDef* timer, uint32_t channel, GPIO_TypeDef* port, uint16_t pin)
		: timer(timer), channel(channel), port(port), pin(pin){
	}
};
